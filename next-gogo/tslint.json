{
  "extends": ["tslint-config-airbnb", "tslint-react", "tslint-config-prettier"],
  "rulesDirectory": ["tslint-plugin-prettier"],
  "rules": {
    "max-line-length": [true, 120],
    "import-name": false,
    "variable-name": false,
    "strict-boolean-expressions": false
  },
  "jsRules": {
    "no-empty": true
  }
}